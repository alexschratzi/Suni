{
  "expo": {
    "name": "Amadeus",
    "slug": "suni",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "suni",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "deploymentTarget": "15.1",
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app shows your timetable using your calendars.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.alexschratzi.suni"
    },
    "extra": {
      "EXPO_PUBLIC_UNI_API_BASE": "https://uni-service.lancealot.at",
      "router": {},
      "eas": {
        "projectId": "545aab06-8dbb-4472-862d-92acab2e1989"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.alexschratzi.suni",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "useModularHeaders": true,
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-localization",
      [
        "@react-native-firebase/app",
        {}
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
